@typeparam T
    <MudContainer Style="max-height: 500px; overflow-y: scroll; padding: 10px 5px 10px 5px">
        <MudExpansionPanels MultiExpansion="true">
            @foreach (var config in Configs)
            {
                <MudExpansionPanel Text="@config.DisplayName">
                    @CreateField(config)
                </MudExpansionPanel>
            }
        </MudExpansionPanels>
    </MudContainer>